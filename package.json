  "scripts": {
    "start": "node ./server/server.js",
    "build": "react-scripts build",

    "start-dev": "concurrently \"npm run client-start\" \"npm run server-start\"",
    "client-start": "react-scripts start",
    "client-test": "react-scripts test",
    "client-eject": "react-scripts eject",

    "server-start": "nodemon --ignore ./sessions ./server/server.js -L",
    "server-test-one:win": "set NODE_ENV=test&& node_modules\\.bin\\mocha  ./server/test/routes/auth-routes.test.js --reporter spec --exit",
    "server-test:win": "set NODE_ENV=test&& node_modules\\.bin\\mocha  ./server/test/**/*.test.js --reporter spec --exit",
    "server-test:lin": "NODE_ENV=test ./node_modules/.bin/mocha ./server/test/**/*.test.js --reporter spec --exit",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha ./server/test/**/*.test.js --recursive --reporter spec --exit",
    "watch-styles-sass": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-date-string": "^0.1.0",
    "chai-http": "^4.2.1",
    "concurrently": "^4.1.0",
