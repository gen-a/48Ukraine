    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "npm-watch": "^0.6.0",
    "prop-types": "^15.7.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "supertest": "^3.4.2"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gen-a/48Ukraine"
  },
  "keywords": [
    "node",
    "heroku",
