{
  "path":"./src/components",
  "templates":{
    "class":"statefulComponent.js",
    "func":"functionalComponent.js",
    "cont":"containerComponent.js",
    "form":"reduxFormComponent.js"
  },
  "default":"func",
  "conventions":{
    "classNameCase":"pascal"
  },
  "replacements":{
    "__authorName__":"",
    "__componentName__":"",
    "__className__":"",
    "__wrapComponentName__":"__wrapComponentName__",
    "__wrapComponentPath__":"__wrapComponentPath__"
  },
  "svg":{
    "sourceDir":"./~source/svg-source",
    "destDir":"./~source/components",
    "svgPropsToRemove":["enableBackground", "xmlSpace", "id", "x", "y" ]
  }
}